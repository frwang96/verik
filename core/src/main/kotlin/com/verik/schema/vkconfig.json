{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "com.verik.schema.vkconfig",
  "type": "object",
  "properties": {
    "sources": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "src": {
            "type": "string"
          },
          "dst": {
            "type": "string"
          }
        },
        "required": ["src", "dst"],
        "additionalProperties": false
      }
    }
  },
  "required": ["sources"],
  "additionalProperties": false
}